{
  "name": "js-utils",
  "version": "0.0.1",
  "description": "Javascript Utils Functions",
  "main": "index.js",
  "scripts": {
    "jshint": "./node_modules/.bin/jshint src/",
    "eslint": "./node_modules/.bin/eslint src/",
    "jscs": "./node_modules/.bin/jscs src/",
    "pretest": "npm run jshint && npm run eslint && npm run jscs",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- -R spec",
    "build:uglify": "./node_modules/.bin/uglifyjs src/jsUtils.js -o index.js -c -mt --preamble //${npm_package_name}@${npm_package_version}",
    "build": "npm run test && npm run build:uglify"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/js-utils.git"
  },
  "keywords": [
    "js",
    "utils"
  ],
  "author": "Vincent Boutour <vincent.boutour@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ViBiOh/js-utils/issues"
  },
  "homepage": "https://github.com/ViBiOh/js-utils",
  "devDependencies": {
    "chai": "latest",
    "chai-as-promised": "latest",
    "eslint": "latest",
    "istanbul": "latest",
    "jscs": "latest",
    "jshint": "latest",
    "mocha": "latest",
    "uglify-js": "latest"
  }
}

{
  "name": "js-utils",
  "version": "1.0.0",
  "description": "Javascript Utils Functions",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"./src/**/*.js\"",
    "style": "eslint --fix \"./src/**/*.js\"",
    "codestyle": "npm run format && npm run style",
    "pretest": "npm run codestyle",
    "test": "nyc --reporter text --reporter lcov --require @babel/register ava --verbose",
    "prebuild": "npm test",
    "build": "babel ./src/jsUtils.js | uglifyjs -o index.js -c -m"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/js-utils.git"
  },
  "keywords": [
    "js",
    "utils"
  ],
  "author": "Vincent Boutour <bob@vibioh.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/js-utils/issues"
  },
  "homepage": "https://github.com/ViBiOh/js-utils",
  "devDependencies": {
    "@babel/cli": "7.8.0",
    "@babel/core": "7.8.0",
    "@babel/preset-env": "7.8.2",
    "@babel/register": "7.7.7",
    "ava": "2.4.0",
    "babel-eslint": "10.0.3",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.9.0",
    "eslint-plugin-import": "2.19.1",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "uglify-js": "3.7.5"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "require": [
      "@babel/register"
    ]
  }
}

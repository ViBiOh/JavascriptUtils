{
  "name": "jsUtils",
  "version": "0.0.1",
  "description": "Javascript Utils Functions",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint src/",
    "checkStyle": "./node_modules/.bin/jscs src/",
    "pretest": "npm run-script lint && npm run-script checkStyle",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive -R spec test/**/*.js",
    "uglify": "./node_modules/.bin/uglifyjs src/primitives.js -o index.js -c --preamble //${npm_package_name}@${npm_package_version}",
    "posttest": "npm run-script uglify"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/JavascriptUtils.git"
  },
  "keywords": [
    "utils",
    "commons"
  ],
  "author": "Vincent Boutour",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ViBiOh/JavascriptUtils/issues"
  },
  "homepage": "https://github.com/ViBiOh/JavascriptUtils",
  "main": "index.js",
  "devDependencies": {
    "chai": "*",
    "chai-as-promised": "*",
    "istanbul": "*",
    "jscs": "*",
    "jshint": "*",
    "mocha": "*",
    "uglify-js": "*"
  }
}

{
  "name": "js-utils",
  "version": "1.0.0",
  "description": "Javascript Utils Functions",
  "main": "index.js",
  "scripts": {
    "prettier": "prettier --write \"./src/**/*.js\"",
    "eslint": "eslint --fix \"./src/**/*.js\"",
    "codestyle": "npm run prettier && npm run eslint",
    "pretest": "npm run codestyle",
    "test": "nyc --reporter text --reporter lcov --require @babel/register ava --verbose",
    "prebuild": "npm test",
    "build": "babel ./src/jsUtils.js | uglifyjs -o index.js -c -m"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/js-utils.git"
  },
  "keywords": [
    "js",
    "utils"
  ],
  "author": "Vincent Boutour <bob@vibioh.fr>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ViBiOh/js-utils/issues"
  },
  "homepage": "https://github.com/ViBiOh/js-utils",
  "devDependencies": {
    "@babel/cli": "7.2.3",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/register": "7.0.0",
    "ava": "1.1.0",
    "babel-eslint": "10.0.1",
    "eslint": "5.12.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.6.0",
    "eslint-plugin-import": "2.14.0",
    "nyc": "13.1.0",
    "prettier": "1.16.0",
    "uglify-js": "3.4.9"
  },
  "ava": {
    "files": [
      "src/**/*.test.js"
    ],
    "require": [
      "@babel/register"
    ]
  }
}

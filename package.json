{
  "name": "jsUtils",
  "version": "0.0.1",
  "description": "Javascript Utils Functions",
  "main": "index.js",
  "scripts": {
    "lint": "./node_modules/.bin/jshint src/",
    "eslint": "./node_modules/.bin/eslint src/",
    "checkStyle": "./node_modules/.bin/jscs src/",
    "pretest": "npm run lint && npm run eslint && npm run checkStyle",
    "test": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --recursive -R spec test/**/*.js",
    "uglify": "./node_modules/.bin/uglifyjs src/primitives.js -o index.js -c --preamble //${npm_package_name}@${npm_package_version}",
    "posttest": "npm run uglify"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ViBiOh/jsUtils.git"
  },
  "keywords": [
    "utils",
    "commons"
  ],
  "author": "Vincent Boutour",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ViBiOh/jsUtils/issues"
  },
  "homepage": "https://github.com/ViBiOh/jsUtils",
  "devDependencies": {
    "chai": "latest",
    "chai-as-promised": "latest",
    "eslint": "latest",
    "istanbul": "latest",
    "jscs": "latest",
    "jshint": "latest",
    "mocha": "latest",
    "uglify-js": "latest"
  }
}
